<h1>Este es el grupo </h1>
<a href="/people/new">Crear Persona</a>

<ul> 
    
    <%@people.each_with_index do |person, id| %>
        <li>
            <%=person.nombre%>
            <a href="/people/<%=id%>">ver</a>
            <a href="/people/<%=id%>/edit">edit</a>
            <form action="/people/<%=id%>" method="post">
                <input type="hidden" name="_method" value="delete">
                <button type="submit"> Eliminar </button>
            </form>
        </li>
    <%end%>
    
</ul>